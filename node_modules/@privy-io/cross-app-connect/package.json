{
  "name": "@privy-io/cross-app-connect",
  "version": "0.2.2",
  "description": "Privy cross app wallet connectors for wagmi and RainbowKit",
  "keywords": [
    "privy",
    "rainbowkit",
    "wagmi"
  ],
  "homepage": "https://docs.privy.io",
  "bugs": {
    "url": "https://privy.io/slack"
  },
  "license": "Apache-2.0",
  "author": "privy.io",
  "sideEffects": false,
  "type": "commonjs",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/dts/index.d.mts",
        "default": "./dist/esm/index.mjs"
      },
      "require": {
        "types": "./dist/dts/index.d.ts",
        "default": "./dist/cjs/index.js"
      }
    },
    "./rainbow-kit": {
      "require": {
        "types": "./dist/dts/rainbow-kit.d.ts",
        "default": "./dist/cjs/rainbow-kit.js"
      },
      "import": {
        "types": "./dist/dts/rainbow-kit.d.mts",
        "default": "./dist/esm/rainbow-kit.mjs"
      }
    }
  },
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.mjs",
  "types": "./dist/dts/index.d.ts",
  "typesVersions": {
    "*": {
      "rainbow-kit": [
        "dist/dts/rainbow-kit.d.ts"
      ]
    }
  },
  "typings": "./dist/dts/index.d.ts",
  "files": [
    "dist/**/*",
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "build:types": "rollup --config rollup.types.mjs",
    "build:code": "rollup --config rollup.build.mjs",
    "check-types": "tsc --noEmit",
    "clean": "rm -rf dist .turbo .swc",
    "dev:code": "rollup --config rollup.dev.mjs --watch",
    "dev:types": "rollup --config rollup.types.mjs --watch",
    "format": "eslint src --fix",
    "lint": "eslint src"
  },
  "browserslist": [
    "defaults",
    "node >= 18",
    "not op_mini all"
  ],
  "dependencies": {
    "@noble/curves": "^1.5.0",
    "@noble/hashes": "1.3.2",
    "@scure/base": "~1.1.2",
    "fflate": "0.8.2"
  },
  "devDependencies": {
    "@privy-io/eslint-config-custom": "*",
    "@privy-io/tsconfig": "*",
    "@rainbow-me/rainbowkit": "^2.2.3",
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-replace": "^5.0.5",
    "@rollup/plugin-swc": "^0.4.0",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^11.1.6",
    "@wagmi/core": "^2.16.4",
    "glob": "^10.3.12",
    "rollup": "^4.28.0",
    "rollup-plugin-copy": "^3.5.0",
    "rollup-plugin-dts": "^6.1.1",
    "typescript": "~5.5.3",
    "viem": "^2.22.23"
  },
  "peerDependencies": {
    "@rainbow-me/rainbowkit": "^2.2.3",
    "@wagmi/core": "^2.16.4",
    "viem": "^2.22.23"
  },
  "peerDependenciesMeta": {
    "@rainbow-me/rainbowkit": {
      "optional": true
    },
    "@wagmi/core": {
      "optional": true
    }
  },
  "publishConfig": {
    "access": "public"
  }
}
